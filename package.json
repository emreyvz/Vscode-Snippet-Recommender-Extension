{
	"name": "snippet-suggest-by-text",
	"displayName": "Snippet Suggest By Text",
	"description": "This extension suggest code snippets by written text that in your own language...",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "EmreYavuz",
	"activationEvents": [
		"onCommand:snippet-suggest-by-text.fetchSnippet"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "snippet-suggest-by-text.fetchSnippet",
				"title": "Get Code Snippet Recommendation"
			}
		],
		"keybindings": [
			{
				"command": "snippet-suggest-by-text.fetchSnippet",
				"key": "ctrl+alt+w",
				"mac": "shift+cmd+w"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"google-it": "^1.6.2",
		"node-fetch": "^3.0.0",
		"serp": "^2.2.1",
		"translate-google": "^1.5.0"
	}
}
